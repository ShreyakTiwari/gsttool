{
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": true,
  "workbench.colorTheme": "Visual Studio Light",
  "window.zoomLevel": -1,
  "editor.tabSize": 2,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.vscode": true
  }
}